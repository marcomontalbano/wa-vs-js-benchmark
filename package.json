{
  "name": "wa-vs-js-benchmark",
  "version": "0.2.0",
  "main": "index.js",
  "author": "Marco Montalbano <marcomontalbano.work@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcomontalbano/wa-vs-js-benchmark"
  },
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "start:dist": "npx serve dist",
    "test:rs": "cargo build --verbose && cargo test --verbose",
    "test:js": "mocha --require @babel/register ./src-js/**/*.test.js",
    "test": "npm run test:rs && npm run test:js",
    "build:wasm": "wasm-pack build",
    "build:rs": "cargo build --release",
    "build:js": "webpack --mode=production --config webpack.config.js",
    "build": "npm run build:wasm && npm run build:js",
    "benchmark": "npm run build:rs && hyperfine --export-markdown BENCHMARK.md  --warmup 3 './target/release/wa-vs-js-benchmark get_primes 100000'",
    "publish": "gh-pages -m 'Publish website.' -b 'gh-pages' -d 'dist' -r \"https://$GH_TOKEN@github.com/marcomontalbano/wa-vs-js-benchmark\""
  },
  "dependencies": {
    "chart.js": "~2.7.3"
  },
  "devDependencies": {
    "@babel/core": "~7.3.3",
    "@babel/preset-env": "~7.3.1",
    "@babel/register": "~7.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "file-loader": "^3.0.1",
    "gh-pages": "~2.0.1",
    "mocha": "~6.0.1",
    "webpack": "~4.29.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "worker-loader": "^2.0.0"
  }
}
